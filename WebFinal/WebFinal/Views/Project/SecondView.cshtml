@model WebFinal.Models.DataForm
@{
    ViewBag.Title = "SecondView";
}

<h2>SecondView</h2>

<div class="form-group">
    @Html.Label("Felicidades tu nombre es ")
    @Html.TextBoxFor(model => model.Name, new { @class = "form-control" })
</div>

<div class="form-group">
    @Html.Label("Felicidades tu correo  es ")
    @Html.TextBoxFor(model => model.Email, new { @class = "form-control" })
</div>

<div class="form-group">
    @Html.Label("Duracion")
    @Html.TextBoxFor(model => model.Duration, new { @class = "form-control" })
</div>

<div class="form-group">
    @Html.Label("Ciudad a donde viajaras ")
    @Html.TextBoxFor(model => model.NameC, new { @class = "form-control" })
</div>

<div class="form-group">
    @Html.Label("ID de Viaje ")
    @Html.TextBoxFor(model => model.City, new { @class = "form-control" })
</div>
